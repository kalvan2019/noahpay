             , t.trans_check_sms
             , t.trans_send_sms
             , t.state
             , t.create_time
             , t.update_time
    </sql>

    <sql id="whereCondition">
        FROM merchant_product_trans t
        WHERE
        <if test="params.merchantNo != null and params.merchantNo.trim() != ''">
            t.merchant_no = #{params.merchantNo} AND
        </if>
        <if test="params.transType != null and params.transType.trim() != ''">
            t.trans_type = #{params.transType} AND
        </if>
        <if test="params.effectiveDate != null and params.effectiveDate.trim() != ''">
            t.effective_date <![CDATA[ >= ]]> #{params.effectiveDate} AND
        </if>
        <if test="params.expiryDate != null and params.expiryDate.trim() != ''">
            t.expiry_date <![CDATA[ <= ]]> #{params.expiryDate} AND
        </if>
        <if test="params.signAuditType != null and params.signAuditType.trim() != ''">
            t.sign_audit_type = #{params.signAuditType} AND
        </if>
        <if test="params.signCheckSms != null and params.signCheckSms.trim() != ''">
            t.sign_check_sms = #{params.signCheckSms} AND
        </if>
        <if test="params.signSendSms != null and params.signSendSms.trim() != ''">
            t.sign_send_sms = #{params.signSendSms} AND
        </if>
        <if test="params.transDealType != null and params.transDealType.trim() != ''">
            t.trans_deal_type = #{params.transDealType} AND
        </if>
        <if test="params.transAuditType != null and params.transAuditType.trim() != ''">
            t.trans_audit_type = #{params.transAuditType} AND
        </if>
        <if test="params.transCheckProtocol != null and params.transCheckProtocol.trim() != ''">
            t.trans_check_protocol = #{params.transCheckProtocol} AND
        </if>
        <if test="params.transCheckSms != null and params.transCheckSms.trim() != ''">
            t.trans_check_sms = #{params.transCheckSms} AND
        </if>
        <if test="params.transSendSms != null and params.transSendSms.trim() != ''">
            t.trans_send_sms = #{params.transSendSms} AND
        </if>
        <if test="params.state != null and params.state.trim() != ''">
            t.state = #{params.state} AND
        </if>
        <if test="params.updateTimeBegin != null and params.updateTimeBegin.trim() != ''">
            t.update_time BETWEEN #{params.updateTimeBegin} AND #{params.updateTimeEnd} AND
        </if>
        1=1
    </sql>

    <select id="queryPage" resultMap="BaseResultMap">
        <include refid="selectAllField"/>
        <include refid="whereCondition"/>
        ORDER BY t.id DESC
