    <sql id="selectAllField">
        SELECT t.id
             , t.merchant_no
             , t.trans_type
             , t.limit_max_amount
             , t.day_limit_amount
             , t.day_limit_number
             , t.month_limit_amount
             , t.month_limit_number
             , t.effective_date
             , t.expiry_date
             , t.sign_audit_type
             , t.sign_check_sms
             , t.sign_send_sms
             , t.trans_deal_type
